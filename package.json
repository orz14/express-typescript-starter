{
  "name": "express-typescript-starter",
  "version": "1.0.0",
  "description": "ExpressJS TypeScript Starter",
  "main": "src/app.ts",
  "scripts": {
    "start": "ts-node ./src/app.ts",
    "dev": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node ./src/app.ts"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "author": "orzcode",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.17",
    "nodemon": "^3.0.3",
    "typescript": "^5.3.3"
  }
}
